{
  "multiprocessing": false,
  "num_cpus": 2,
  "num_gpus": 0.25,
  "gamma": 0.99,
  "batch_size": 32,
  "replay_size": 80000,
  "learning_rate": 0.0001,
  "sync_target_frames": 5000,
  "replay_start_size": 10000,
  "epsilon_decay_last_frame": 200000,
  "epsilon_start": 1.0,
  "epsilon_final": 0.01,
  "n_steps": 2,
  "save_checkpoints": false,
  "use_double": false,
  "random_seed": 4321,

  "runs": [
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_1step_double",
      "use_double": true,
      "train_frames": 1000000,
      "n_steps": 1
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_2step_double",
      "use_double": true,
      "train_frames": 1000000,
      "n_steps": 2
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_1step_double_noisy",
      "use_double": true,
      "use_dense": "noisy",
      "train_frames": 1000000,
      "n_steps": 1
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_2step_double_noisy",
      "use_double": true,
      "use_dense": "noisy",
      "train_frames": 1000000,
      "n_steps": 2
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_1step_double_factorized_noisy",
      "use_double": true,
      "use_dense": "factorized_noisy",
      "train_frames": 1000000,
      "n_steps": 1
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_2step_double_factorized_noisy",
      "use_double": true,
      "use_dense": "factorized_noisy",
      "train_frames": 1000000,
      "n_steps": 2
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_1step_noisy",
      "use_double": false,
      "use_dense": "noisy",
      "train_frames": 1000000,
      "n_steps": 1
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_2step_noisy",
      "use_double": false,
      "use_dense": "noisy",
      "train_frames": 1000000,
      "n_steps": 2
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_1step_factorized_noisy",
      "use_double": false,
      "use_dense": "factorized_noisy",
      "train_frames": 1000000,
      "n_steps": 1
    },
    {
      "env": "PongNoFrameskip-v4",
      "run_name": "Atari_2step_factorized_noisy",
      "use_double": false,
      "use_dense": "factorized_noisy",
      "train_frames": 1000000,
      "n_steps": 2
    }
  ]
}
